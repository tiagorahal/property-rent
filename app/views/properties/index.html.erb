<div class="row">
  <div class="col-sm-12 col-md-6 d-md-flex flex-row flex-wrap justify-contente-aroud align-items-center mb-5" id="properties">
    <% @properties.each do |property| %>
      <div class="col m-auto card-with-button">
        <%= render property %>        
      </div>
    <% end %>
  </div>
  <iframe
    class="col-sm-12 col-md-6" 
    src="https://www.google.com/maps/embed"
    width="600"
    height="800"
    style="border:0;"
    allowfullscreen=""
    loading="lazy"
  />
</div>